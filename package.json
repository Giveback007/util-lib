    "source-map-support": "^0.5.16",
    "stack-trace": "^0.0.10",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^13.5.0",
    "@types/source-map-support": "^0.5.1",
    "@types/stack-trace": "^0.0.29",
    "concurrently": "^5.0.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
