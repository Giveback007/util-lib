{
  "name": "@giveback007/util-lib",
  "version": "0.22.6",
  "description": "Small util function library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "publish-script": "del dist /s /f /q && npm run lint && npm run build && npm publish",
    "test": "jest --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "playground:node": "node playground.js && parcel playground/node-index.ts --target node --cache-dir playground/node-dist",
    "playground:web": "node playground.js && parcel playground/index.html --cache-dir playground/dist"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giveback007/util-lib.git"
  },
  "keywords": [],
  "author": "Dovydas Stirpeika <dovy91@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Giveback007/util-lib/issues"
  },
  "homepage": "https://github.com/Giveback007/util-lib#readme",
  "dependencies": {
    "tslib": "~2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.11",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.30.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  }
