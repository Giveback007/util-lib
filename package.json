{
  "name": "@giveback007/util-lib",
  "version": "0.6.11",
  "description": "Small util function library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "publish-script": "del dist /s /f /q && npm run lint && npm run build && npm publish",
    "build": "tsc",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "playground": "concurrently \"npm:playground:tsc\" \"npm:playground:nodemon\" -k --kill-others-on-fail -p \"{name}|\" -n \"TSC ,NODE\" -c \"cyan,green\"",
    "playground:tsc": "tsc --watch -p playground.tsconfig.json",
    "playground:nodemon": "node_modules/.bin/nodemon --inspect-brk=9230 playground/dist/playground/index.js"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giveback007/util-lib.git"
  },
  "keywords": [],
  "author": "Dovydas Stirpeika <dovy91@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Giveback007/util-lib/issues"
  },
  "homepage": "https://github.com/Giveback007/util-lib#readme",
  "dependencies": {
    "colors": "1.3.3",
    "source-map-support": "^0.5.11",
    "stack-trace": "0.0.10",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/stack-trace": "0.0.29",
    "concurrently": "4.1.0",
    "nodemon": "1.18.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "nodemonConfig": {
    "watch": [
      "dist",
      "playground/dist"
    ]
  }
}
