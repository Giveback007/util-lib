  "homepage": "https://github.com/Giveback007/util-lib#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "react-dom": "^16.12.1",
    "react": "^16.12.1",
    "tslib": "^1.11.1",
    "moment-timezone": "^0.5.28"
  },
  "devDependencies": {
    "@types/jest": "^24.1.4",
    "@types/moment-timezone": "^0.5.13",
