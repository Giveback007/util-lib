{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["es2017", "dom"],
        "outDir": "playground/dist/",
        "pretty": true,
        "sourceMap": true,
        "importHelpers": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strict": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noFallthroughCasesInSwitch": false,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "moduleResolution": "node"
    },
    "files": [
        "./playground/index.ts"
    ],
    "exclude": [
        "./playground/dist/"
    ]
    
}
